{
  "name": "audiovizualizer",
  "version": "1.0.0",
  "description": "audio viz",
  "main": "index.js",
  "scripts": {
    "buildts": "tsc",
    "build": "rollup -c",
    "rollup:watch": "rollup -c -w",
    "vite:watch": "vite",
    "server": "serve dist",
    "start:rollup": "run-p rollup:watch server",
    "start": "npm run vite:watch",
    "deploy-client": "firebase deploy",
    "deploy": "npm run build && npm run deploy-client",
    "cp:electron": "cp ./electron/* ./dist",
    "electron": "electron ./dist/main.js",
    "start:electron": "run-p cp:electron build:watch electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlemagnussen/audioviz.git"
  },
  "keywords": [
    "web",
    "audio",
    "visualizer"
  ],
  "author": "atlemagnussen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlemagnussen/audioviz/issues"
  },
  "homepage": "https://github.com/atlemagnussen/audioviz#readme",
  "dependencies": {
    "@material/mwc-button": "^0.25.3",
    "@material/mwc-icon": "^0.25.3",
    "@material/mwc-select": "^0.25.3",
    "lit": "^2.0.2",
    "rxjs": "^7.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@web/rollup-plugin-html": "^1.9.1",
    "electron": "^15.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-lit": "^1.5.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.55.1",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^1.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^12.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.6.13"
  }
}
